{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "import-name": false,
  "variable-name": false,
  "whitespace": [
    true,
    "check-type",
    "check-branch",
    "check-operator",
    "check-decl",
    "check-module",
    "check-rest-spread",
    "check-typecast",
    "check-type-operator"
  ],
  "align": true,
  "ter-arrow-parens": false,
  "function-name": false
}